<%- include('adminHeader.ejs') -%>
    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4 roleMain">
        <div class="studentTitle d-flex w-100 justify-content-between mb-5">
            <h2>Add student</h2>
            <a href="/"><button type="button" class="btn btn-dark">Dark</button></a>
        </div>
        <div class="table-responsive">
            <table class="table table-striped table-sm">
                <thead>
                    <tr>
                        <th>srno</th>
                        <th>Email</th>
                        <th>Password</th>
                        <th>Semester</th>
                        <th>Header</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(let i = 0; i < result.length; i++){ %>
                        <tr>
                            <td>
                                <%= i %>
                            </td>
                            <td>
                                <%= result[i].email %>
                            </td>
                            <td>
                                <%= result[i].password %>
                            </td>
                            <td>
                                <%= result[i].sem %>
                            </td>
                            <td>sit</td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
    </main>
    <%- include('adminFooter.ejs') -%>